<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Radpostauth" table="radpostauth">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="64" nullable="false"/>
    <field name="pass" type="string" column="pass" length="64" nullable="false"/>
    <field name="reply" type="string" column="reply" length="32" nullable="false"/>
    <field name="authdate" type="datetime" column="authdate" nullable="false"/>
  </entity>
</doctrine-mapping>
